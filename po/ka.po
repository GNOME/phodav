#: bin/chezdav.c:120
msgid "Print program version"
msgstr "პროგრამის ვერსიის დაბეჭდვა"

#: bin/chezdav.c:121
msgid "Be verbose"
msgstr "დამატებითი შეტყობინებები"

#: bin/chezdav.c:122
msgid "Port to listen to"
msgstr "მოსასმენი პორტი"

#: bin/chezdav.c:123
msgid "Listen on loopback only"
msgstr "მხოლოდ lo ინტერფეისზე მოსმენა"

#: bin/chezdav.c:124
msgid "Listen on all interfaces"
msgstr "ყველა ინტერფეისზე მოსმენა"

#: bin/chezdav.c:125
msgid "Path to export"
msgstr "გასატანილი ბილიკი"

#: bin/chezdav.c:126
msgid "Path to htdigest file"
msgstr "ბილიკი htdigest failamde"

#: bin/chezdav.c:127
msgid "DIGEST realm"
msgstr "DIGEST რეალმი"

#: bin/chezdav.c:128
msgid "Read-only access"
msgstr "მხოლოდ-წაკითხვის წვდომა"

#: bin/chezdav.c:130
msgid "Skip mDNS service announcement"
msgstr "\"mDNS\" სერვისის გამოცხადების გამოტოვება"

#: bin/chezdav.c:142
msgid "- simple WebDAV server"
msgstr "- მარტივი WebDAV სერვერი"

#: bin/chezdav.c:143
#, c-format
msgid "Report bugs to <%s>"
msgstr "შეცდომების შესახებ მიწერეთ: <%s>"

#: bin/chezdav.c:150
#, c-format
msgid "Option parsing failed: %s\n"
msgstr "პარამეტრების დამუშავების შეცდომა: %s\n"

#: bin/chezdav.c:154
#, c-format
msgid "Unsupported extra arguments: %s ...\n"
msgstr "მხარდაუჭერელი დამატებითი არგუმენტები: %s …\n"

#: bin/chezdav.c:163
msgid "--local and --public are mutually exclusive\n"
msgstr "--local და --public ურთიერთგამომრიცხავია\n"

#: bin/chezdav.c:189
#, c-format
msgid "Failed to open htdigest: %s\n"
