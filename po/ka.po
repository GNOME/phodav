#: bin/chezdav.c:204
#, c-format
msgid "mDNS failed: %s\n"
msgstr "mDNS-ის შეცდომა: %s\n"

#: bin/chezdav.c:215
msgid "Internal error, should not happen\n"
msgstr "შიდა შეცდომა. არ უნდა ხდებოდეს\n"

#: bin/chezdav.c:218
#, c-format
msgid "Listen failed: %s\n"
msgstr "მოსმენის შეცდომა: %s\n"
