#: bin/chezdav.c:116
msgid "Print program version"
msgstr "Вивести дані щодо версії програми"

#: bin/chezdav.c:117
msgid "Be verbose"
msgstr "Режим докладних повідомлень"

#: bin/chezdav.c:118
msgid "Port to listen to"
msgstr "Порт, на якому слід очікувати на дані"

#: bin/chezdav.c:119
msgid "Listen on loopback only"
msgstr "Очікувати дані лише на петльовому інтерфейсі"

#: bin/chezdav.c:120
msgid "Listen on all interfaces"
msgstr "Очікувати дані на усіх інтерфейсах"

#: bin/chezdav.c:121
msgid "Path to export"
msgstr "Шлях для експортування"

#: bin/chezdav.c:122
msgid "Path to htdigest file"
msgstr "Шлях до файла htdigest"

#: bin/chezdav.c:123
msgid "Read-only access"
msgstr "Доступ лише до читання"

#: bin/chezdav.c:134
msgid "- simple WebDAV server"
msgstr "– простий сервер WebDAV"

#: bin/chezdav.c:135
#, c-format
msgid "Report bugs to <%s>"
msgstr "Адреса для повідомлень про вади: <%s>"

#: bin/chezdav.c:142
#, c-format
msgid "Option parsing failed: %s\n"
msgstr "Не вдалося обробити параметри: %s\n"

#: bin/chezdav.c:146
#, c-format
msgid "Unsupported extra arguments: %s ...\n"
msgstr "Непідтримувані зайві аргументи: %s ...\n"

#: bin/chezdav.c:155
msgid "--local and --public are mutually exclusive\n"
msgstr "Не можна одночасно використовувати --local і --public\n"

#: bin/chezdav.c:179
#, c-format
msgid "Failed to open htdigest: %s\n"
msgstr "Не вдалося відкрити htdigest: %s\n"

#: bin/chezdav.c:197
#, c-format
msgid "mDNS failed: %s\n"
msgstr "Помилка mDNS: %s\n"

#: bin/chezdav.c:208
msgid "Internal error, should not happen\n"
msgstr "Внутрішня помилка, цього не мало трапитися\n"

#: bin/chezdav.c:211
#, c-format
msgid "Listen failed: %s\n"
msgstr "Не вдалося розпочати очікування на дані: %s\n"
